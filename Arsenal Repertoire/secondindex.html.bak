    <style>
        body {
            margin: 0px;
            padding: 0px;
        }
        
        .headerFont {
            font-family: 'Ubuntu', sans-serif;
            font-size: 24px;
        }
        
        .subFont {
            font-family: 'Raleway', sans-serif;
            font-size: 14px;
        }
        
        .specialHead {
            font-family: 'Oswald', sans-serif;
        }
        
        .normalFont {
            font-family: 'Roboto Condensed', sans-serif;
        }
        
        a {
            color: #FFFFFF;
            text-decoration: none;
        }
        
        a:link {
            color: #FFFFFF;
            text-decoration: none;
        }
        /* visited link */
        
        a:visited {
            color: #FFFFFF;
            text-decoration: none;
        }
        /* mouse over link */
        
        a:hover {
            color: #FFFFFF;
            text-decoration: none;
        }
        /* selected link */
        
        a:active {
            color: #FFFFFF;
            text-decoration: none;
        }

        .army {
            position: relative;
            min-height: 1px;
            padding-right: 15px;
            padding-left: 15px;
        }

        .image {
            opacity: 1;
            display: block;
            width: 250px
            height: 150px;
            transition: .5s ease;
            backface-visibility: hidden;
        }   

        .middle {
            transition: .5s ease;
            opacity: 0;
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
            text-align:center;
        }

        .army:hover .image {
            opacity: 0.3;
        }

        .army:hover .middle {
            opacity: 1;
        }

        .text {
            background-color: #04AA6D;
            color: white;
            font-size: 16px;
            padding: 16px 32px;
        }
		
		.service-box{
            width: 100%;
			display: flex;
			flex-wrap: wrap;
			justify-content: space-around;
			margin: auto;
		}
		
		.single-service{
            flex-basis: 30%;
			text-align: center;
			border-radius: 7px;
			margin-bottom: 0px;
			color:#fff;
			position: relative;
		}
		.single-service img{
			width: 85%;
			height: 85%;
			border-radius: 7px;

			
		}
		
		.overlay{
			width: 100%;
			height: 85%;
			position: absolute;
			top: 0;
			border-radius: 7px;
			cursor: pointer;
			background: linear-gradient(rgba(128,128,128),#FFFFFF);
			opacity: 0;
			transition: 1s;

		}
		
		.army-desc{
			width: 30%;
			position: absolute;
			bottom: 0%;
			left: 50%;
			transform: translateX(-50%);
			opacity: 0;
			transition: 1s;
			
		}
		
		
		.single-service:hover  .overlay{
			opacity:0.5;
			bottom: 40%;
  
			}
			
		hr{
			background: #fff;
			height: 2px;
			border: 0;
			margin: 15px auto;
			width: 60%;
		}

		
		body {
           background-image: url('images/download.jfif');
           background-repeat: no-repeat;
           background-attachment: fixed;
           background-size: 100% 100%;
        }

    </style>


